{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "one-drive-carousel-adaptive-card-extension": {
      "components": [
        {
          "entrypoint": "./lib/adaptiveCardExtensions/oneDriveCarousel/OneDriveCarouselAdaptiveCardExtension.js",
          "manifest": "./src/adaptiveCardExtensions/oneDriveCarousel/OneDriveCarouselAdaptiveCardExtension.manifest.json"
        }
      ]
    },
    "one-drive-full-image-carousel-adaptive-card-extension": {
      "components": [
        {
          "entrypoint": "./lib/adaptiveCardExtensions/oneDriveFullImageCarousel/OneDriveFullImageCarouselAdaptiveCardExtension.js",
          "manifest": "./src/adaptiveCardExtensions/oneDriveFullImageCarousel/OneDriveFullImageCarouselAdaptiveCardExtension.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "OneDriveCarouselAdaptiveCardExtensionStrings": "lib/adaptiveCardExtensions/oneDriveCarousel/loc/{locale}.js",
    "OneDriveFullImageCarouselAdaptiveCardExtensionStrings": "lib/adaptiveCardExtensions/oneDriveFullImageCarousel/loc/{locale}.js"
  }
}
